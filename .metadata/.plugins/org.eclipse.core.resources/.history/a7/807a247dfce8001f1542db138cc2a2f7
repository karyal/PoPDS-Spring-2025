package week5.day3;

public class Person {
	private int pid;
	private String fullName;
	//Constructors
	public Person() {
		this.pid=0;
		this.fullName="";
	}
	public Person(int pid, String fullName) {
		this.pid=pid;
		this.fullName=fullName;
	}
	//Setters
	public void setPID(int pid) {
		this.pid=pid;
	}
	public void setFullName(String fullName) {
		this.fullName=fullName;
	}
	//Getters
}
