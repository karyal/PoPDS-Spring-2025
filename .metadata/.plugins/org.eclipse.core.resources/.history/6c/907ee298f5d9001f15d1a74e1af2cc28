package week2.day5;
//WAP which accept two numbers from user, calculate sum, and display.

import java.util.Scanner;
public class Task1 {

	public static void main(String[] args) {
		//declare
		   String fristno;
		   String secondno;
		   int x;
		   int y;
		   int sum;
		   
		   //input
		   System.out.println("Enter first number");
		   fristno = new Scanner(System.in).nextLine();
		   System.out.println("Enter Second number");
		   secondno = new Scanner(System.in).nextLine();
		   
		   //process
		   x= Integer.parseInt(fristno);    
		   y= Integer.parseInt(secondno);
		   
		   sum= x+y;
		   
		   //output
		   System.out.println("Sum of two number :"+ sum);
		       
		   }
	}
}
