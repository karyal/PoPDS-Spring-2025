package working.nested_classes;

class Class1 {
	public void f1() {
		System.out.println("Hello from f1() of Class1");
	}
	
	class Class2 {
		public void f1() {
			System.out.println("Hello from f1() of Class1-> Class2");
		}
	}
	
	public static void main(String[] args) {
		new Class1().f1();
		Class1.Class2() obj2 = new Class1.Class2();
	}
}